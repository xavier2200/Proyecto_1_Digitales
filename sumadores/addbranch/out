:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x55c1a1d701c0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55c1a1d70350 .scope module, "sumadorb_tb" "sumadorb_tb" 3 3;
 .timescale -9 -12;
P_0x55c1a1d842c0 .param/l "N" 0 3 6, +C4<00000000000000000000000000000100>;
v0x55c1a1dbe900_0 .var "a_i", 3 0;
v0x55c1a1dbe9f0_0 .var "b_i", 3 0;
v0x55c1a1dbeac0_0 .net "sum_o_actual", 3 0, L_0x55c1a1dbec60;  1 drivers
v0x55c1a1dbebc0_0 .var "sum_o_expected", 3 0;
S_0x55c1a1dac2d0 .scope function.void, "check_output" "check_output" 3 51, 3 51 0, S_0x55c1a1d70350;
 .timescale -9 -12;
TD_sumadorb_tb.check_output ;
    %load/vec4 v0x55c1a1dbeac0_0;
    %load/vec4 v0x55c1a1dbebc0_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 6;
    %vpi_call/w 3 53 "$display", "Test passed: sum_o_actual = sum_o_expected = %b", v0x55c1a1dbeac0_0 {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call/w 3 55 "$error", "Test failed: sum_o_actual = %b, sum_o_expected = %b", v0x55c1a1dbeac0_0, v0x55c1a1dbebc0_0 {0 0 0};
T_0.1 ;
    %end;
S_0x55c1a1dac4b0 .scope module, "dut" "sumadorb" 3 16, 4 4 0, S_0x55c1a1d70350;
 .timescale -9 -12;
    .port_info 0 /INPUT 4 "a_i";
    .port_info 1 /INPUT 4 "b_i";
    .port_info 2 /OUTPUT 4 "sum_o";
P_0x55c1a1dac690 .param/l "N" 0 4 4, +C4<00000000000000000000000000000100>;
v0x55c1a1d99260_0 .net "a_i", 3 0, v0x55c1a1dbe900_0;  1 drivers
v0x55c1a1dbe6b0_0 .net "b_i", 3 0, v0x55c1a1dbe9f0_0;  1 drivers
v0x55c1a1dbe790_0 .net "sum_o", 3 0, L_0x55c1a1dbec60;  alias, 1 drivers
L_0x55c1a1dbec60 .arith/sum 4, v0x55c1a1dbe900_0, v0x55c1a1dbe9f0_0;
    .scope S_0x55c1a1d70350;
T_1 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55c1a1dbe900_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55c1a1dbe9f0_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55c1a1dbebc0_0, 0, 4;
    %callf/void TD_sumadorb_tb.check_output, S_0x55c1a1dac2d0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x55c1a1dbe900_0, 0, 4;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x55c1a1dbe9f0_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x55c1a1dbebc0_0, 0, 4;
    %callf/void TD_sumadorb_tb.check_output, S_0x55c1a1dac2d0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x55c1a1dbe900_0, 0, 4;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x55c1a1dbe9f0_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 14, 0, 4;
    %store/vec4 v0x55c1a1dbebc0_0, 0, 4;
    %callf/void TD_sumadorb_tb.check_output, S_0x55c1a1dac2d0;
    %vpi_call/w 3 47 "$finish" {0 0 0};
    %end;
    .thread T_1;
